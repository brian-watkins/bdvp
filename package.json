{
  "name": "esbehavior",
  "version": "1.1.0",
  "description": "A framework for writing executable documentation",
  "homepage": "https://github.com/brian-watkins/esbehavior/tree/main/packages/esbehavior",
  "repository": {
    "type": "git",
    "url": "https://github.com/brian-watkins/esbehavior"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "type": "module",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf ./dist && tsc",
    "test": "node --loader ts-node/esm --no-warnings ./test/index.ts",
    "test:one": "node --loader ts-node/esm --no-warnings ",
    "sample:node": "npm run test:raw --prefix samples/node",
    "sample:react": "npm run test --prefix samples/react"
  },
  "author": "Brian Watkins",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/node": "^17.0.21",
    "chai": "^4.3.4",
    "esm": "^3.2.25",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "uvu": "^0.5.1"
  }
}
