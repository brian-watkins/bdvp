  },
  "devDependencies": {
    "@types/node": "^15.12.4",
    "@types/proclaim": "^3.6.2",
    "esbehavior": "^1.0.0",
    "proclaim": "^3.6.0",
    "tap-mocha-reporter": "^5.0.1",
    "tap-spec": "^5.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
